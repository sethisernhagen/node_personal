doctype html
html
  head
    meta(charset='utf-8')
    meta(name='viewport', content='width=device-width, initial-scale=1.0')
    title #{title} | Seth Isernhagen
    link(rel='shortcut icon', href='/images/favicon.png')
    link(rel='stylesheet', type='text/css', href='https://maxcdn.bootstrapcdn.com/bootstrap/3.3.4/css/bootstrap.min.css')
    link(rel='stylesheet', type='text/css', href='/stylesheets/style.css')
    link(href="https://fonts.googleapis.com/css?family=Pontano+Sans", rel="stylesheet", type="text/css")
    script(src='/javascripts/modernizr-2.6.2.js')
  body
    .navbar.navbar-inverse.navbar-fixed-top
      .container
        .navbar-header
          button.navbar-toggle(type='button', data-toggle='collapse', data-target='.navbar-collapse')
            span.icon-bar
            span.icon-bar
            span.icon-bar
          a.navbar-brand(href='/') Seth Isernhagen
        .navbar-collapse.collapse
          ul.nav.navbar-nav
            li
              a(href='/about') About
            li.dropdown
                a(href="#",class="dropdown-toggle", data-toggle="dropdown",role="button",aria-expanded="false") Work
                        span.caret
                ul(class="dropdown-menu",role="menu")
                    li
                        a(href="/quote") D3.js Stock Quote Line Charts
                    li
                        a(href="/bubbleChart") D3.js Pack Layout with Zoom
            li
              a(href='/contact') Contact
    block content

    script(src='https://ajax.googleapis.com/ajax/libs/jquery/1.11.2/jquery.min.js')
    script(src='https://maxcdn.bootstrapcdn.com/bootstrap/3.3.4/js/bootstrap.min.js')
    //-script(src='/javascripts/respond.js')

    block scripts
